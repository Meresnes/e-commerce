{
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@config/*": [
        "src/config/*"
      ],
      "@styles/*": [
        "src/styles/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@type/*": [
        "src/type/*"
      ],
      "@store/*": [
        "src/store/*"
      ],
      "@models/*": [
        "src/models/*"
      ],
      "@service/*": [
        "src/service/*"
      ]
    }
  }
}